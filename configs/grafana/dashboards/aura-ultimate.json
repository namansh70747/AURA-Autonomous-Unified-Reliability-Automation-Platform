        {
            "id": 12,
            "title": "Recent Kubernetes Events",
            "type": "table",
            "gridPos": {
                "x": 0,
                "y": 30,
                "w": 24,
                "h": 10
            },
            "datasource": {
                "type": "postgres",
                "uid": "PostgreSQL"
            },
            "targets": [
                {
                    "rawSql": "SELECT \n  timestamp AT TIME ZONE 'UTC' as time,\n  COALESCE(event_type, 'UNKNOWN') as event_type,\n  COALESCE(pod_name, 'N/A') as pod_name,\n  COALESCE(namespace, 'default') as namespace,\n  COALESCE(message, 'No message') as message\nFROM events \nWHERE timestamp > NOW() - INTERVAL '1 hour'\nORDER BY timestamp DESC \nLIMIT 50",
                    "format": "table",
                    "refId": "A"
                }
            ],
            "options": {
                "showHeader": true,
                "sortBy": [
                    {
                        "displayName": "time",
                        "desc": true
                    }
                ],
                "footer": {
                    "show": true,
                    "reducer": [
                        "count"
                    ]
                }
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "align": "left",
                        "filterable": true
